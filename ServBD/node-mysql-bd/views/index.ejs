<%- include('partials/header.ejs'); %>
	<div class="container">
		<div class="table-wrapper">
				<table class="table table-hovered">
					<thead class="thead-dark">
						<tr>
							<th scope="col">ID</th>
							<th scope="col">Nome</th>
							<th scope="col">Partido</th>
							<th scope="col">UF</th>
							<th scope="col">Titularidade</th>
							<th scope="col">Email</th>
							<th scope="col">DtNasc</th>
						</tr>
					</thead>
					<tbody>
						<% senadores.forEach((senadores, index) => { %>
							<tr>
								<td><%= senadores.ID%></td>
								<td><%= senadores.Nome %></td>
								<td><%= senadores.Partido %></td>
								<td><%= senadores.UF %></td>
								<td><%= senadores.Titularidade %></td>
								<td><%= senadores.Email %></td>
								<td><%= senadores.DtNasc %></td>
							</tr>
						<% }) %>
					</tbody>
				</table>
		</div>
    </div>
</div>
</body>
<canvas id="myChart" width="400" height="400"></canvas>
<script>
var ctx = document.getElementById('myChart').getContext('2d');
var myChart = new Chart(ctx, {
    type: 'pie',
    data: {
        labels: ['Red', 'Blue', 'Yellow', 'Green', 'Purple', 'Orange'],
        datasets: [{
            label: '# of Votes',
            data: [12, 19, 3, 5, 2, 3],
            backgroundColor: [
                'rgba(255, 99, 132, 0.2)',
                'rgba(54, 162, 235, 0.2)',
                'rgba(255, 206, 86, 0.2)',
                'rgba(75, 192, 192, 0.2)',
                'rgba(153, 102, 255, 0.2)',
                'rgba(255, 159, 64, 0.2)'
            ],
            borderColor: [
                'rgba(255, 99, 132, 1)',
                'rgba(54, 162, 235, 1)',
                'rgba(255, 206, 86, 1)',
                'rgba(75, 192, 192, 1)',
                'rgba(153, 102, 255, 1)',
                'rgba(255, 159, 64, 1)'
            ],
            borderWidth: 1
        }]
    },
    options: {
        scales: {
            yAxes: [{
                ticks: {
                    beginAtZero: true
                }
            }]
        }
    }
});
</script>
</html>
